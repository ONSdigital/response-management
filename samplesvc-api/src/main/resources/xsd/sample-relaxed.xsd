            <xsd:element name="frotover" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="turnover" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="entref" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="wowentref" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="vatref" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="payeref" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="crn" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="live_lu" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="live_vat" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="live_paye" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="legalstatus" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="entrepmkr" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="birthdate" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="entname1" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="entname2" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="entname3" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="runame1" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="runame2" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="runame3" type="xsd:string" minOccurs="0" maxOccurs="1" />
